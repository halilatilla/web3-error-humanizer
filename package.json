{
  "name": "web3-error-humanizer",
  "version": "1.0.0",
  "description": "Humanize cryptic Web3 and DEX errors using AI and local heuristics.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --watch --dts",
    "test": "vitest",
    "test:local": "tsx examples/test-local.ts"
  },
  "keywords": [
    "web3",
    "ethereum",
    "dex",
    "error",
    "humanize",
    "openai",
    "viem"
  ],
  "author": "halilatilla",
  "license": "MIT",
  "dependencies": {
    "openai": "^4.0.0",
    "viem": "^2.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  }
}
